{
  "name": "ai-ui-components",
  "type": "module",
  "version": "0.0.0-beta.4",
  "sideEffects": true,
  "scripts": {
    "start": "vite --config ./vite.config.dev.js",
    "build": "rm -rf dist && rollup -c",
    "format": "prettier -w .",
    "release": "npm run build && npm publish ./dist"
  },
  "exports": {
    "./Chatbot": {
      "import": "./Chatbot.js",
      "require": "./Chatbot.js",
      "types": "./Chatbot.d.ts"
    },
    "./FormChatbot": {
      "import": "./FormChatbot.js",
      "require": "./FormChatbot.js",
      "types": "./FormChatbot.d.ts"
    },
    "./JournalChatbot": {
      "import": "./JournalChatbot.js",
      "require": "./JournalChatbot.js",
      "types": "./JournalChatbot.d.ts"
    },
    "./Tree": {
      "import": "./Tree.js",
      "require": "./Tree.js",
      "types": "./Tree.d.ts"
    },
    "./TreeChatbot": {
      "import": "./TreeChatbot.js",
      "require": "./TreeChatbot.js",
      "types": "./TreeChatbot.d.ts"
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "http-server": "^14.1.1",
    "openai": "^5.9.0",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-modal": "^3.16.3",
    "rollup": "^4.45.1",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^7.0.0"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "lodash-es": "^4.17.21",
    "react-icons": "^5.5.0",
    "react-tooltip": "^5.29.1",
    "style-inject": "^0.3.0"
  }
